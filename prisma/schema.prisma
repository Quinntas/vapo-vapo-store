// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database.db"
}

model User {
  id          String @id @default(cuid())
  publicId    String @default(uuid())
  phoneNumber String
  email       String @unique
  password    String
  fullName    String
  Address     String
  createdOn   String
  modifiedOn  String
}